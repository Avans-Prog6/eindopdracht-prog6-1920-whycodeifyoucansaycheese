@model BeestjeOpJeFeestje.Domain.Models.BookingVM
@{
    Layout = null;
}
<div class="container">
    <h3>Bookings overzicht</h3>
    <p>Datum:</p>
    <p>@Model.Date</p>
    <p>Beesten:</p>
    <ul>
        @foreach (var beast in Model.Beast)
        {
            <li> @beast.Name </li>
        }
    </ul>
    <p>Accessoires:</p>
    <ul>
        @foreach (var acc in Model.Accessory)
        {
            <li>@acc.Name</li>
        }
    </ul>
</div>